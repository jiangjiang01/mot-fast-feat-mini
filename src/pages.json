{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "fast feat"
      }
    },
    {
      "path": "pages/featA/index",
      "style": {
        "navigationBarTitleText": "feat a"
      }
    }
  ],
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^xt-(.*)": "@/components/base/xt-$1/Index.vue"
    }
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "fast feat",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
